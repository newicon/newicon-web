		"laravel-mix": "^5.0.4",
		"postcss-custom-properties": "^9.1.1",
		"postcss-import": "^12.0.1",
		"vue-template-compiler": "^2.6.11",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@tailwindcss/ui": "^0.1.3",
		"postcss": "^8.2.10",
